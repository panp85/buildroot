@HAVE_INTROSPECTION_TRUE@			;; \
@HAVE_INTROSPECTION_TRUE@		esac; \
@HAVE_INTROSPECTION_TRUE@	done

@HAVE_INTROSPECTION_TRUE@$(top_builddir)/build/win32/$(1).msvc.introspect:
@HAVE_INTROSPECTION_TRUE@	-$(RM) $(top_builddir)/build/win32/$(1).msvc.introspect

